<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pattern.Controller.OverviewController">
    <children>
        <ListView fx:id="productListView" layoutX="50" layoutY="50" prefHeight="300" prefWidth="500" />
        <Button fx:id="detailButton" layoutX="466.0" layoutY="360.0" onAction="#handleDetailButton" text="Show Details" />
      <Label layoutX="261.0" layoutY="14.0" text="Overview">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="teruGaan" layoutX="50.0" layoutY="360.0" onAction="#teruGaan" mnemonicParsing="false" prefHeight="26.0" prefWidth="66.0" text="Terug" />
    </children>
</AnchorPane>
