<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pattern.Controller.OverviewController">
    <children>
        <ListView fx:id="productListView" layoutX="50" layoutY="50" prefHeight="300" prefWidth="500" />
        <Button fx:id="detailButton" layoutX="550" layoutY="360" text="Show Details" onAction="#handleDetailButton"/>
    </children>
</AnchorPane>
